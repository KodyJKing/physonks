    "devDependencies": {
        "@glas/test": "^1.0.4",
        "@types/node": "^15.12.5",
        "ava": "^3.15.0",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.0.0-beta.2",
        "push-dir": "^0.4.1",
        "typescript": "4.2.4"
    }
}