    "description": "",
    "main": "index.js",
    "scripts": {
        "watch1": "npm-run-all --parallel watch1:*",
        "watch2": "npm-run-all --parallel watch2:*",
        "watch1:parcel": "parcel --public-url ./ src/demos/index.html",
